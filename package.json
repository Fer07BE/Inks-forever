    "node": "^19.8.1",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.62.1"
  },
  "scripts": {
    "compile:sass": "node-sass Sass/main.scss style.css",
    "devserver": "live-server",
    "watch:sass": "node-sass Sass/main.scss style.css -w",
    "build:css": "npm-run-all compile:sass",
